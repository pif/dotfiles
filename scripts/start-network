MYDIR="$(dirname "$(readlink -f "$0")")"
$MYDIR/clear-network

ifconfig eth0 192.168.1.122 netmask 255.255.255.0
route add -net default gw 192.168.1.27 dev eth0
echo "# Generated by NetworkManager\n\
nameserver 8.8.8.8\n\
nameserver 8.8.4.4" > /etc/resolv.conf

arp -s 192.168.1.27 00:02:b3:4c:91:76
